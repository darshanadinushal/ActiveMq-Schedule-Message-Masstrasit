{
  "Application": {
    "Name": "Sample.Application.Consumer"
  },

  "ActiveMQ": {
    "QueueSettings": {
      "HostName": "<host-name>",
      "UserName": "<activemq username>",
      "Password": "<activemq password>",
      "Port": "61617",
      "ClaimSubmission": {
        "RetryCount": 3,
        "Interval": 5000
      }
    }
  },
  "SmtpEmailProvider": {
    "SmtpClientHost": "in-v3.mailjet.com",
    "From": "<email-smtp- from-email>",
    "UserName": "<email-smtp- from-usename>",
    "Password": "<email-smtp- from-password>",
    "Port": "587"
  },
  "AllowedHosts": "*"

}
